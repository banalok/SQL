CREATE TABLE store(id INTEGER PRIMARY KEY, items TEXT, quantity INTEGER, consumer TEXT, current_availability TEXT, price INTEGER);
INSERT INTO store VALUES(1, "Notebook", 50, "Kids and Adults", "A", 10);
INSERT INTO store VALUES(2, "Tablet", 25, "Adults", "A", 500);
INSERT INTO store VALUES(3, "Bike", 28, "Kids", "N/A", 100);
INSERT INTO store VALUES(4, "Cellphone", 100, "Adults", "N/A", 350);
INSERT INTO store VALUES(5, "Ball", 25, "Kids", "A", 5);

SELECT items, price FROM store ORDER BY price;
SELECT MAX(price) FROM store;
SELECT SUM(price) FROM store;
SELECT SUM(quantity) FROM store;
